                  <items count="11">
                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import rhinoscriptsyntax as rs
import ghpythonlib.components as ghpc


crv_src = NURBS_CURVE
resolution = int(RESOLUTION)

crv_out = []


def nurbs_to_polyline(crv_guid, res):
    
    crv = rs.coercecurve(crv_guid)
    
    len_ = ghpc.Length(crv)

    split_count = int(len_ / res)
    # print(split_count)
    
    pts, t0, t1 = ghpc.DivideCurve(crv, split_count, False)
    
    pl = ghpc.PolyLine(pts, False)
    
    ### Down Sampling
    pl_ds, _smp = ghpc.SimplifyCurve(pl, 10)
    
    
    return pl_ds


for i in xrange(len(crv_src)):
    
    pl = nurbs_to_polyline(crv_src[i], resolution)
    crv_out.append(pl)

POLYLINES = crv_out</item>
