                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import rhinoscriptsyntax as rs
import ghpythonlib.components as ghpc


def join_item(list):
    
    txt = ",".join(list)
    txt += "\n"
    return txt


def join_list(list):
    
    txt = "".join(list)
    return txt


def xyz_to_uv(pt):
    
    ### Convert Rhino_XYZ to Screen_UV
    
    point = rs.coerce3dpoint(pt)
    # print(point)
    
    u = str(int(abs(point.X)))
    # print(point.X, u)

    v = str(int(abs(point.Y)))
    # print(point.Y, v)
    
    return u, v


def polyline_to_uv(polylines):
    
    uv_list = []
    dbg = []
    joined = None
    
    for i in xrange(len(polylines)):
        
        verts = rs.PolylineVertices(pls[i])
        dbg.append(verts)
    
        sub = []
        
        for j in xrange(len(verts)):
            
            uu, vv = xyz_to_uv(verts[j])
            sub.append(uu)
            sub.append(vv)
        
        ### List to String
        sub_join = join_item(sub)
        # print(sub_join)
        
        uv_list.append(sub_join)
        
    return uv_list, dbg


def merge_uv_with_color(uv_list, color):
    
    merged = []
    
    for i in xrange(len(uv_list)):
        # print(i, uv_list[i])
        
        ### Create
        txt = "{}, {}".format(clr, uv_list[i])
#        print(txt)
        
        merged.append(txt)
    
    merged_join = join_list(merged)
    # print("Export :", merged_join)
    
    return merged_join


def export(path, txt):

    ### Export
    with open(path, 'w') as f:
        f.write(txt)



export_data = []
pls = POLYLINES
clr = COLOR_ID

calc_points, debug = polyline_to_uv(pls)
