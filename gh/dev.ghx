                    <item name="CodeInput" type_name="gh_string" type_code="10">import rhinoscriptsyntax as rs


def draw_pt(pt, vecx, count):
    
    pts = []
    
    for i in xrange(count):
        
        pts.append(rs.PointAdd(pt, rs.CreateVector(vecx * i, 0, 0)))
        
    return pts


def join_item(list):
    
    txt = ",".join(list)
    txt += "\n"
    return txt


def join_list(list):
    
    txt = "".join(list)
    return txt


def xyz_to_uv(pt):
    
    ### Convert Rhino_XYZ to Screen_UV
    
    point = rs.coerce3dpoint(pt)
    # print(point)
    
    u = str(int(abs(point.X)))
    # print(point.X, u)

    v = str(int(abs(point.Y)))
    # print(point.Y, v)
    
    return u, v



def define_tool(pt):
    
    tool_pos = xyz_to_uv(pt)
    # print(tool_pos)
    
    return tool_pos


def define_color(pt):
    
    color_pos = xyz_to_uv(pt)
    # print(color_pos)
    
    return color_pos


def define(tool_pt, color_pt, color_length):
    
    define_out = []
    
    ### Define Tool
    tool_uv = define_tool(tool_pt)
    tool_uv_f = "uv_tool, {}, {}\n".format(tool_uv[0], tool_uv[1])
    define_out.append(tool_uv_f)
    
    ### Define Color    
    for i in xrange(8):
        point = rs.coerce3dpoint(color_pt)
        
        ### Calc Colors
        color_point = rs.AddPoint(point.X + color_length * i, point.Y, point.Z)
        color_uv = define_color(color_point)
        color_uv_f = "uv_color_{}, {}, {}\n".format(i+1, color_uv[0], color_uv[1])
        define_out.append(color_uv_f)
    
    define_joined = join_list(define_out)
    
    return define_joined


def export(path, txt):

    ### Export
    with open(path, 'w') as f:
        f.write(txt)



### Set Path
path = r"C:\Users\yoshioca\Documents\Tajima_Sai_Automation\data\tools.txt"
